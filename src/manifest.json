{
  "name": "Gaia",
  "short_name": "Gaia",
  "description": "Gaia Labs presenting privacy model for ChatGpt",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Gaia"
  },
  "host_permissions": [
    "https://chat.openai.com/chat"
  ],
  "options_page": "index.html?#/options",
  "background": {
    "service_worker": "extension/background/background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "*"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline'"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "cookies",
    "webNavigation",
    "identity"
  ],
  "icons": {
    "512": "/assets/icons/android-chrome-512x512.png",
    "192": "/assets/icons/android-chrome-192x192.png",
    "32": "/assets/icons/favicon-32x32.png",
    "16": "/assets/icons/favicon-16x16.png"
  },
  "storage": {
    "managed": {
      "api_key": {
        "value": "your_api_key_here"
      }
    }
  }
}
